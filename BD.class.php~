class BD{
  private $file;
  private $tables;

  function __construct($file){  
    $this->setfile($file);
  }

  function setFile($file){
    $this->file = $file;
  }

  function createTable($name, $fields){
    $table = new CreateTable();
    if (!$table->setName($name, $this->getTableName()))
      return FALSE;
    $table->setFields($fields);
  }
}
